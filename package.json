{
  "name": "ironmike",
  "main": "app/main.js",
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "nodemon": "set NODE_ENV=development&& nodemon ./tests/web.js",
    "dev": "npm-run-all -p webpack:watch nodemon",
    "start-js": "webpack-dev-server",
    "webpack:watch": "webpack -d --watch --config ./webpack/webpack.config.dev.js",
    "build-css": "node-sass-chokidar app/frontend/styles/ -o app/public/css/",
    "watch-css": "npm run build-css && node-sass-chokidar app/frontend/styles/ -o app/public/css/ --watch --recursive"
  },
  "dependencies": {
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "bottleneck": "1.16.0",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "get-port": "^4.0.0",
    "glob": "^7.1.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.10",
    "lowdb": "^1.0.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "opn": "^5.3.0",
    "react-dom": "^16.4.2",
    "react-json-view": "^1.19.1",
    "react-toastify": "^4.1.0",
    "reflux": "^6.4.1",
    "reverse-tunnel-ssh": "^1.1.0",
    "rimraf": "^2.6.2",
    "rocky": "^0.4.15",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.82.1",
    "serve-favicon": "^2.5.0",
    "slugify": "^1.3.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "ssh2": "^0.6.1",
    "sweetalert2": "^7.26.10",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "node-sass": "^4.9.2",
    "react": "^16.4.2",
